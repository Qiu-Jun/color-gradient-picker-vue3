{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@l/*": ["./*"],
      "@c/*": ["./components/*"]
    }
  },
  "include": [
    "./**/*.ts",
    "./**/*.d.ts",
    "./**/*.vue",
    "./**/*.tsx",
    "./dts/*.d.ts",
    "./index.d.ts",
    "./types/*.ts",
    "./auto-imports.d.ts"
  ]
}
